(function(a,b,c){b.extend(c,{quickInit:function(){if(b.isIos){var d=b(".playBar");d.each(function(e){var f=b(this);if(!f.attr("ipvurl")){f.remove()}})}if(c.autoBanner){b.imgScroll({showType:0,autoScroll:true,isLoop:true,delayTime:3000,imgBigList:b(".photo_b_list"),leftBtn:b(".photo_big_left"),rightBtn:b(".photo_big_right")});b(".photo_box").hover(function(){b(".photo_big_bar").show()},function(){b(".photo_big_bar").hide()})}},myInit:function(){var d=this;if(c.autoMidBanner){b.imgScroll({showType:3,autoScroll:true,isLoop:true,delayTime:3000,imgBigList:b(".photo_b_list_2"),leftBtn:b(".photo_big_left_2"),rightBtn:b(".photo_big_right_2")})}b(".bodyMain").delegate(".playBar","click",function(){d.view(b.isIos?this.getAttribute("ipvurl"):this.getAttribute("pvurl"))});b(".bodyMain").delegate(".want_see","click",function(){c.wantToSee(b(this))});b(".upMovieTab").click(this.mvTabSwitch);this.initEmlEvent()},mvTabSwitch:function(){var e=b(this),d=e.attr("rel");if(e.hasClass("active")){return}b(".upMovieTab").removeClass("active");e.addClass("active");b(".upMovie").addClass("hide");b("#"+d).removeClass("hide")}})})(window,jQuery,Core);